
public class Worker {
	private double pay;
	private String name;


	public Worker(double pay, String name) {
		this.pay = pay;
		this.name = name;
	}


	public void setPay(double pay) {
		this.pay = pay;
	}


	public double getPay() {
		return pay;
	}



	public String getName() {
		return name;
	}



}
